{
  "entrypoint": "public/index.html",
  "shell": "public/src/app-shell.html",
  "fragments": [
    "public/src/app-view.html",
    "public/src/app-login.html",
    "public/src/app-view404.html"
  ],
  "sources": [
    "public/src/**/*",
    "public/images/**/*"
  ],
  "extraDependencies": [
    "public/manifest.json",
    "public/appconfig.json",
    "public/bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled",
      "basePath": true
    },
    {
      "preset": "es6-unbundled",
      "basePath": true
    }
  ]
}
